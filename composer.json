{
  "name": "yam/info-card",
  "version": "2.0.0",
  "description": "Modernized Info Card for Laravel Nova 5.0 with beautiful gradients",
  "keywords": [
    "laravel",
    "nova",
    "card",
    "dashboard",
    "admin"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "YAM - You Are Marvelous",
      "email": "info@yam.berlin"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Yam\\InfoCard\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yam\\InfoCard\\CardServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}